    "dotenv": "^4.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "fs-extra": "^5.0.0",
    "https": "^1.0.0",
    "jest-localstorage-mock": "^2.1.0",
    "jsonwebtoken": "^8.1.0",
    "leaflet": "^1.2.0",
    "morgan": "^1.9.0",
    "object-assign": "^4.1.1",
    "pdfjs-dist": "^2.0.303",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
